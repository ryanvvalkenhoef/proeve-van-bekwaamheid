{
    "require": {
        "hassankhan/config": "~2.2",
        "illuminate/database": "~8.0",
        "ircmaxell/random-lib": "~1.2",
        "php-di/php-di": "~6.3",
        "phpmailer/phpmailer": "~6.2",
        "selective/basepath": "~2.0",
        "slim/psr7": "^1.3",
        "slim/slim": "~4.0",
        "slim/views": "0.1.*",
        "twig/twig": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
}
